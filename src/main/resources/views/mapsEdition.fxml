<?import javafx.scene.layout.RowConstraints?>

<fx:root id="centerPane" fx:id="mapsEditionGridPane" prefHeight="730.0" prefWidth="1210.0" stylesheets="@../css/custom.css" type="javafx.scene.layout.GridPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="stories.mapsedition.MapsEditionController">
   <children>
      <Button fx:id="importMapsFromServer" mnemonicParsing="false" onAction="#importMapsFromServerOnAction" prefWidth="180.0" text="Import server's maps" GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
      <Button fx:id="searchInWorkShop" mnemonicParsing="false" onAction="#searchInWorkShopOnAction" prefWidth="180.0" text="Search in WorkShop" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP">
         <GridPane.margin>
            <Insets top="10.0" />
         </GridPane.margin>
      </Button>
      <Button fx:id="selectAllMaps" mnemonicParsing="false" onAction="#selectAllMapsOnAction" prefWidth="180.0" text="Select all maps" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="TOP">
         <GridPane.margin>
            <Insets top="10.0" />
         </GridPane.margin>
      </Button>
      <TextField fx:id="searchMaps" maxWidth="210.0" onKeyReleased="#searchMapsKeyReleased" GridPane.valignment="CENTER" />
      <ImageView fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true" GridPane.halignment="LEFT" GridPane.valignment="CENTER">
         <image>
            <Image url="@../images/find.png" />
         </image>
         <GridPane.margin>
            <Insets left="210.0" />
         </GridPane.margin>
      </ImageView>
      <Button fx:id="addNewMaps" mnemonicParsing="false" onAction="#addNewMapsOnAction" prefWidth="180.0" text="Add new maps" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
         <GridPane.margin>
            <Insets />
         </GridPane.margin></Button>
      <Label text="View" GridPane.columnIndex="4" GridPane.halignment="RIGHT">
         <GridPane.margin>
            <Insets right="270.0" />
         </GridPane.margin></Label>
      <Button fx:id="removeMaps" alignment="CENTER" mnemonicParsing="false" onAction="#removeMapsOnAction" prefWidth="180.0" text="Delete selected maps" GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
         <GridPane.margin>
            <Insets />
         </GridPane.margin></Button>
      <ComboBox fx:id="viewPaneCombo" onAction="#viewPaneComboOnAction" prefWidth="260.0" GridPane.columnIndex="4" GridPane.halignment="RIGHT" GridPane.valignment="CENTER">
         <GridPane.margin>
            <Insets left="60.0" />
         </GridPane.margin></ComboBox>
      <Label text="Size" GridPane.columnIndex="4" GridPane.halignment="RIGHT" GridPane.rowIndex="1">
         <GridPane.margin>
            <Insets bottom="25.0" right="270.0" />
         </GridPane.margin>
      </Label>
      <Slider fx:id="mapsSlider" blockIncrement="25.0" maxWidth="240.0" minorTickCount="2" onMouseClicked="#mapsSliderOnMouseClicked" showTickLabels="true" showTickMarks="true" snapToTicks="true" value="50.0" GridPane.columnIndex="4" GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.rowIndex="1">
         <padding>
            <Insets left="-25.0" right="5.0" top="5.0" />
         </padding>
         <GridPane.margin>
            <Insets />
         </GridPane.margin></Slider>
      <Label id="titleConfigLabel" fx:id="officialMapsLabel" text="Official Maps" GridPane.columnSpan="5" GridPane.halignment="LEFT" GridPane.rowIndex="1" GridPane.valignment="BOTTOM">
         <GridPane.margin>
            <Insets top="8.0" />
         </GridPane.margin></Label>
      <Label id="titleConfigLabel" fx:id="customMapsLabel" text="Custom Maps" GridPane.columnSpan="5" GridPane.halignment="LEFT" GridPane.rowIndex="3" GridPane.valignment="TOP">
         <GridPane.margin>
            <Insets top="8.0" />
         </GridPane.margin></Label>
      <ScrollPane fx:id="officialMapsScrollPane" fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" vbarPolicy="ALWAYS" GridPane.columnSpan="5" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="BOTTOM">
         <content>
            <FlowPane fx:id="officialMapsFlowPane" hgap="50.0" style="-fx-background-color: black;" vgap="50.0">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></FlowPane>
         </content>
         <GridPane.margin>
            <Insets bottom="5.0" />
         </GridPane.margin>
      </ScrollPane>
      <ScrollPane fx:id="customMapsScrollPane" fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" vbarPolicy="ALWAYS" GridPane.columnSpan="5" GridPane.rowIndex="4">
         <content>
            <FlowPane fx:id="customMapsFlowPane" hgap="50.0" style="-fx-background-color: black;;" vgap="50.0">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding></FlowPane>
         </content>
         <GridPane.margin>
            <Insets bottom="5.0" />
         </GridPane.margin>
      </ScrollPane>
   </children>
   <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" minWidth="240.0" prefWidth="1210.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="216.0" prefWidth="1210.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="216.0" prefWidth="1210.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="217.0" prefWidth="1210.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="320.0" prefWidth="1210.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="50.0" prefHeight="100.0" />
      <RowConstraints minHeight="70.0" />
      <RowConstraints minHeight="10.0" prefHeight="730.0" />
      <RowConstraints minHeight="50.0" />
      <RowConstraints minHeight="10.0" prefHeight="730.0" />
   </rowConstraints>
</fx:root>
